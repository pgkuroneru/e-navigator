<h1>ユーザー一覧</h1>
<table class="table table-bordered">
  <thead class="thead-light">
  <tr>
    <th scope="col">名前</th>
    <th scope="col">Email</th>
    <th scope="col">年齢</th>
    <th scope="col">性別</th>
    <th scope="col">学校名</th>
  </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.sex %></td>
        <td><%= user.school_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
